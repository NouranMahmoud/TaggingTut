{
  "bail": false,
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "(src/(__tests__|__ssr_tests__)/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
  "moduleDirectories": ["src", "node_modules"],
  "mapCoverage": true
}
